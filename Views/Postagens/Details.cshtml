@model CreditaNelas.Models.Postagens

@{
    ViewData["Title"] = "Details";
}


<section class="details-body">
    <div class="container">
        <h4 class="text-center">Seus Projetos</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Id_Usuario)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Id_Usuario)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Titulo)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Titulo)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Whatsapp)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Whatsapp)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Descricao)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Descricao)
            </dd>
        </dl>
    </div>
</section>

<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id_Postagem"><i class="fa-solid fa-pen"></i> Editar</a>
    <a class="btn btn-primary" asp-action="Index"><i class="fa-solid fa-table-list"></i> Voltar para lista</a>
</div>
